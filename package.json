{
  "name": "ctructure",
  "displayName": "Ctructure",
  "description": "An opinionated code formatter for the C programming language, inspired by Prettier.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:Ctructure.formatCurrentFile"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Ctructure.formatCurrentFile",
        "title": "Ctructure: Format Current File"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "testCore": "npx jest",
    "testCoreWatch": "npx jest --watchAll",
    "preExtTest": "npm run compile-tests && npm run compile && npm run lint",
    "compileExtTests": "tsc -p . --outDir out",
    "testExt": "node ./out/test/runTest.js",
    "testExtWatch": "tsc -p . -w --outDir out"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "glob": "^7.1.7",
    "jest": "^27.4.5",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  }
}
